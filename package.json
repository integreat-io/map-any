{
  "name": "map-any",
  "version": "0.2.1",
  "description": "Map mappables (functors) and simple objects (non-functors)",
  "author": "Kjell-Morten Bratsberg Thorsen <post@kjellmorten.no>",
  "license": "ISC",
  "keywords": [
    "map",
    "javascript",
    "functional"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./async.js": {
      "import": "./dist/async.js",
      "types": "./dist/async.d.ts"
    }
  },
  "scripts": {
    "test": "npm run lint && npm run build && c8 --reporter=text-summary ava --config ./ava-dist.config.cjs",
    "test:watch": "npm run dev",
    "dev": "ava --watch",
    "build": "tsc",
    "prepare": "npm run build",
    "coverage": "c8 report",
    "lint": "eslint --ext .ts src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjellmorten/map-any.git"
  },
  "bugs": {
    "url": "https://github.com/kjellmorten/map-any/issues"
  },
  "homepage": "https://github.com/kjellmorten/map-any#readme",
  "engines": {
    "node": ">= 16.20"
  },
  "devDependencies": {
    "@integreat/ts-dev-setup": "^4.1.1"
  }
}
