{
  "name": "map-any",
  "version": "0.1.2",
  "description": "Map mappables (functors) and simple objects (non-functors)",
  "author": "Kjell-Morten Bratsberg Thorsen <post@kjellmorten.no>",
  "license": "ISC",
  "keywords": [
    "map",
    "javascript",
    "functional"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "tsc && nyc --reporter=text-summary ava dist/**/*-test.js",
    "test:watch": "npm run dev",
    "dev": "ava --watch",
    "build": "tsc",
    "prepare": "npm run build",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "nyc:report": "nyc report",
    "lint": "tslint --project tsconfig.json --config tslint.json"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "files": [
      "src/**/*-test.ts"
    ],
    "sources": [
      "src/**/!(*-test).ts"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*-test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjellmorten/map-any.git"
  },
  "bugs": {
    "url": "https://github.com/kjellmorten/map-any/issues"
  },
  "homepage": "https://github.com/kjellmorten/map-any#readme",
  "devDependencies": {
    "ava": "1.0.0-beta.6",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.2",
    "nyc": "^12.0.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^3.0.1"
  }
}
